{
  "private": true,
  "name": "web-app",
  "version": "1.0.0",
  "license": "AGPL",
  "scripts": {
    "develop": "ENV_ID=dev npm run -s dev",
    "build-clean": "rm -rf public/*",
    "build": "preact build --src src --template src/index.html --no-prerender --dest build",
    "build-move": "mv build/* public",
